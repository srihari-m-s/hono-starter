{
  "name": "hono-starter",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "cross-env DB_MIGRATING=true npx drizzle-kit migrate",
    "db:seed": "cross-env DB_SEEDING=true tsx src/db/seed.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@hono/node-server": "^1.14.1",
    "@hono/zod-openapi": "^0.19.5",
    "@scalar/hono-api-reference": "^0.8.2",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-orm": "^0.35.3",
    "drizzle-zod": "^0.5.1",
    "globals": "^16.0.0",
    "hono": "^4.7.7",
    "hono-pino": "^0.8.0",
    "i18next": "^25.0.0",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "postgres": "^3.4.4",
    "stoker": "^1.2.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/css": "^0.7.0",
    "@eslint/js": "^9.25.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.17",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}